project T5_Graph is

   for Main use ("t5_graph.adb");
   for Source_Dirs use ("src");
   for Object_Dir use "bin";

   package Compiler is
      for Default_Switches ("ada") use ("-g", "-gnato", "-gnatwa", "-gnatwe", "-gnatQ", "-gnat12");
   end Compiler;

   package Builder is
      for Default_Switches ("ada") use ("-g");
   end Builder;

end T5_Graph;