project T3_vectors is
   for Main use ("t3_vectors.adb");
   for Source_Dirs use ("src");
   for Object_Dir use "bin";

   package Compiler is
      for Default_Switches ("ada") use ("-g", "-gnato",
                                        "-gnatwa", "-gnatwe",
                                        "-gnatQ", "-gnat12");
   end Compiler;

   package Builder is
      for Default_Switches ("ada") use ("-g");
   end Builder;
end T3_vectors;
