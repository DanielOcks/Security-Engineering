project T5_Threads is

   for Source_Dirs use ("src/aunit/", "src");
   for Object_Dir use "bin";
   for Main use ("t5_threads.adb");

   package Compiler is
      for Default_Switches ("ada") use ("-g", "-gnato", "-gnatwa",
                                     --  "-gnatwe",
                                        "-gnatQ", "-gnat12", "-gnata");
   end Compiler;

   package Builder is
      for Default_Switches ("ada") use ("-g");
   end Builder;

end T5_Threads;
